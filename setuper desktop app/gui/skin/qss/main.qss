/***********************************************************/
/* QMainWindow Style*/
/***********************************************************/
QMainWindow{
    color: rgb(0, 0, 255);
}

QFrame#TitleBar{
    background-color: transparent;
}

QFrame QLabel#TitleLabel{
    color: white;
    font-family: 'Verdana';
    font-size: 12px;
    padding-left: 10px;
}

QFrame#NavgationBar{
    background-color: transparent;
    border-bottom: 2px solid white;
}

QFrame#QWebBrowserPage{
    background-color: transparent;
    border-bottom: none;
}

QToolBar {
    background: transparent;
    spacing: 0px; /* spacing between items in the tool bar */
}

QFrame#HomePage{
    background: white;
}

QFrame#BrowsePage{
    border:20px solid #1ABC9C;
}

QFrame#UploadPage{
    background-color: white;
}

QFrame#SetuperPage{
    color: white;
    background-color: #1ABC9C;
}

QFrame#SetuperPage QLabel{
    color: white;
    font-size: 16px;
}

QFrame#SetuperPage QLineEdit{
    color: #1ABC9C;
    font-size: 16px;
    border-radius: 2px;
}

QFrame#AboutPage{
    background-color: white;
}

QLabel#Count{
    border-radius: 50px;
    background-color: #1ABC9C;
    color: white;
}

QProgressBar {
    border: 1px solid white;
    text-align: right;
}

QProgressBar::chunk {
    background-color: #16A085;
    width: 20px;
}

QProgressBar QLineEdit{
    border: none;
    color: black;
    padding-left: 5px;
    font-size: 16px;
    background-color: transparent;
}

QMenu {
    color: white;
    /*background-image: url(gui/skin/PNG/bg_dock.png);*/
    margin: 2px; /* some spacing around the menu */
}

QMenu::item {
    height: 23px;
    padding: 0px 25px 0px 20px;
    /*background-image: url(gui/skin/PNG/bg_dock.png);*/
    background: rgb(54, 178, 230);
}

QMenu::item:selected {
    background-color: #16A085;
}

QMenu::icon:checked { /* appearance of a 'checked' icon */
    background: #16A085;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
 }

QMenu::item:disabled
{
    background: rgb(125, 125, 125);
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}


QMenu::separator {
    height: 2px;
    background: lightblue;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 13px;
    height: 13px;
}

QStatusBar {
    color: black;
    background-color: transparent;
    border-left: 1px solid #1ABC9C;
    border-right: 1px solid #1ABC9C;
    border-bottom: 1px solid #1ABC9C;
}

QStatusBar QLabel#datatimelabel{
    background-color: transparent;
    font: 16px;
    color: black;
}

QToolButton{
    background-color: transparent;
    color: white;
}


QToolButton:hover{
    background-color: #16A085;
    border: 1px;
}

QToolButton:disabled{
    background-color: gray;
    border: 1px;
}

QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 0px; /* make way for the popup button */
}

QToolButton::menu-button {
    border: none;
}

QToolButton::menu-arrow {
    image: none;
}


QToolButton#close{
    background-color: transparent;
    color: white;
}


QToolButton#close:hover{
    background-color: red;
    border: 1px;
}


QSplitter{
    border:none;
}

QSplitter::handle {
    background-color: gray;
}

QSplitter::handle:horizontal {
    width: 3px;
}

QSplitter::handle:vertical {
    height: 3px;
}

QSplitter::handle:pressed {
    background-color: green;
}

QPushButton#FlatUIButton{
    background-color: #58d68d;
    color: white;
    text-align: center;
}

QPushButton#FlatUIButton:hover{
    background-color: #48c9b0;
}

QPushButton#FlatUIButton:pressed{
    background-color: #1abc9c;
}

QPushButton#FlatUIButton:flat{
    border: none;
}


QPushButton#customColorButton{
    color: rgb(255, 255, 255);
    background-color: white;
    background: green;
    border:none;
    font-size: 15px;
    text-align: center;
    width: 60px;
}
QPushButton#customColorButton:hover, QPushButton#customColorButton:pressed
{
    background-color: rgb(85, 170, 255);
    text-align: center;
}


